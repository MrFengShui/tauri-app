{
    "build": {
        "beforeDevCommand": "npm run build",
        "beforeBuildCommand": "npm run build",
        "devPath": "../docs/simplified-chinese",
        "distDir": "../docs/simplified-chinese"
    },
    "package": {
        "productName": "tauri-app",
        "version": "0.0.0"
    },
    "tauri": {
        "allowlist": {
            "all": false,
            "shell": {
                "all": false,
                "open": true
            }
        },
        "windows": [
            {
                "title": "tauri-app",
                "minimizable": true,
                "maximizable": true,
                "maximized": true,
                "width": 1920,
                "height": 1080
            }
        ],
        "security": {
            "csp": null
        },
        "bundle": {
            "active": true,
            "targets": "all",
            "identifier": "com.tauri.dev",
            "icon": [
                "icons/32x32.png",
                "icons/128x128.png",
                "icons/128x128@2x.png",
                "icons/icon.icns",
                "icons/icon.ico"
            ]
        }
    }
}